// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Image {
  id    Int     @id @default(autoincrement())
  name  String  @unique
  tags  Tag[]
}

model Tag {
  id        Int     @id @default(autoincrement())
  name      String  @unique
  images    Image[]
}
